<% include partials/header %>
  <h1 class="text-center">Welcome to the Blog App</h1>
  <% blogs.forEach((blog,index)=>{ %>
      <div class="container">
        <h2><%= blog.title %></h2>
        <img src="#" alt="<%= blog.image %>">
        <p class="lead"><%= blog.body.substring(0, 100) %>....</p>
        <p><%= blog.created.toDateString() %></p>
        <a href="/blogs/<%= blog._id %>">Show More</a>
      </div>
  <% }) %>
<% include partials/footer %>

  
   
