<% include partials/header %>
<div class="container">
  <div class="row">
    <div class="col-6 offset-3">
      <form action="/blogs" method="POST">

      <div class="form-group">
        <input id="title" class="form-control" type="text" name="title" placeholder="title">
      </div>

      <div class="form-group">
        <input id="image" class="form-control" type="text" name="image" placeholder="image">
      </div>

      <div class="form-group">
        <textarea class="form-control" type="text" name="body" placeholder="body"></textarea>
      </div>

        <input class="btn btn-block btn-outline-primary" type="submit">
      </form>
    </div>
  </div>
</div>
<% include partials/footer %>